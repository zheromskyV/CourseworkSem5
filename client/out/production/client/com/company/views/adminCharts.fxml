<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="900.0" prefWidth="1200.0" style="-fx-background-color: f6f4e6;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.controllers.AdminChartsController">
   <children>
      <AnchorPane prefHeight="150.0" prefWidth="1200.0" style="-fx-background-color: fddb3a;">
         <children>
            <Text fill="#41444b" layoutX="333.0" layoutY="99.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Построить графики">
               <font>
                  <Font name="Arial" size="60.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
      <Button fx:id="backBtn" focusTraversable="false" layoutX="20.0" layoutY="170.0" minHeight="40.0" mnemonicParsing="false" onAction="#onBack" prefWidth="120.0" text="&lt; Назад">
         <font>
            <Font name="Arial" size="20.0" />
         </font>
      </Button>
      <LineChart fx:id="chartAccred" layoutX="20.0" layoutY="250.0" prefHeight="400.0" prefWidth="560.0" title="По аккредитации">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <BarChart fx:id="chartSatisf" layoutX="620.0" layoutY="250.0" prefHeight="400.0" prefWidth="560.0" title="По удовлетворенности">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
